<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav id="sitenav">
        @Html.ActionLink("LionFish", "Index", "Home", new { area = "" }, new { id = "lionfish", @class = "" })
        <div id="snb">
            @Html.ActionLink("About", "About", "Home", null, new { @class = "button" })
            @Html.ActionLink("Help", "Help", "Home", null, new { @class = "button" })
            <a class="button getstarted" style="border:1px solid #fff;">get started</a>
        </div>
    </nav>
    <div class="content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/Index.js")
    @RenderSection("scripts", required: false)
</body>
</html>
