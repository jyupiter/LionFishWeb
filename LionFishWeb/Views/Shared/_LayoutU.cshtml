<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/landing.css")
    @Styles.Render("~/Content/tippy.css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav id="sitenav">
        @Html.ActionLink("LionFish", "Index", "Home", new { area = "" }, new { id = "lionfish", @class = "" })
        <div id="snb">
            @Html.ActionLink("Help", "Help", "Home", null, new { @class = "button" })
            @Html.ActionLink("Get started", "SignUp", "Home", new { area = "" }, new { id = "getstarted", @class = "button" })
        </div>
    </nav>
    <div class="content">
        @RenderBody()
    </div>
    s
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/Index.js")
    @Scripts.Render("~/Scripts/tippy.js")
    @RenderSection("scripts", required: false)
    <script src='https://www.google.com/recaptcha/api.js' type="text/javascript"></script>
</body>
</html>
